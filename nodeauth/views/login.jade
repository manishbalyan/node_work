extends layout

block content
    h1 Login
    p Please login here
    form(method="post", action="/user/login", enctype="multipart/form-data")
        .form-group
            input.form-control(type="text",name="username", placeholder="Enter Username" )
        .form-group
            input.form-control(type="password",name="password", placeholder="Enter Password" )
        input.btn.btn-primary(type="submit", name="submit", value="Login")